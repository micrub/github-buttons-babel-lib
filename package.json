{
  "private": true,
  "name": "@michaelr/github-buttons-babel-lib",
  "version": "0.0.7",
  "description": "Github-buttons Unofficial Babel starter kit based module distribution library source code repository.",
  "license": "MIT",
  "keywords": [],
  "main": "index.js",
  "jsnext:main": "index.es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/micrub/github-buttons-babel-lib"
  },
  "babel": {
    "presets": [
      "latest",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      [
        "transform-strict-mode",
        {
          "strict": false
        }
      ]
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "browserify": "^14.4.0",
    "chai": "^4.0.0-canary.1",
    "del": "^3.0.0",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.1.2",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^2.6.1"
  },
  "scripts": {
    "lint": "eslint src test",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch",
    "build": "node tools/build",
    "publish": "cd dist/ && npm publish",
    "prepublish": "npm run build"
  }
}
